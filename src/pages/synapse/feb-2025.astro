    <!-- Images of magazine pages -->
    <div class="grid gap-6">
      <img src="/images/synapse/feb-2025-pg1.jpg" alt="Synapse Feb 2025 Page 1" class="rounded-lg shadow" />
      <img src="/images/synapse/feb-2025-pg2.jpg" alt="Synapse Feb 2025 Page 2" class="rounded-lg shadow" />
      <img src="/images/synapse/feb-2025-pg3.jpg" alt="Synapse Feb 2025 Page 3" class="rounded-lg shadow" />
    </div>
  </main>
